{
  "name": "stanford_basic",
  "version": "1.0.0",
  "repository": "https://github.com/SU-SWS/stanford_basic",
  "config": {
    "srcSass": "src/scss",
    "distSass": "dist/css",
    "srcJS": "src/js",
    "distJS": "dist/js",
    "srcAssets": "src/assets",
    "distAssets": "dist/assets"
  },
  "scripts": {
    "build": "webpack --mode=development",
    "publish": "webpack --mode=production",
    "watch": "webpack --mode=development --watch",
