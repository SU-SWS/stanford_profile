{
  "name": "stanford_profile_styles",
  "version": "0.1.0",
  "repository": "https://github.com/SU-SWS/stanford_profile",
  "config": {
    "srcSass": "lib/scss",
    "distSass": "dist/css",
    "srcJS": "lib/js",
    "distJS": "dist/js",
    "srcAssets": "lib/assets",
    "distAssets": "dist/assets"
  },
  "scripts": {
    "build": "webpack --mode=development --display-error-details",
    "publish": "webpack --mode=production",
    "watch": "webpack --mode=development --watch",
    "lint:js": "eslint -c .eslintrc 'src/js'",
    "lint:sass": "sass-lint -c .sass-lint.yml '**/*.scss' -v -q -i 'vendor/**/*.scss, node_modules/**/*.scss'"
  },
  "devDependencies": {
    "@babel/core": "^7.6",
    "@babel/preset-env": "^7.6",
    "acorn": "^6.1.1",
    "autoprefixer": "^9.4.10",
    "babel-loader": "^8.0.5",
    "bourbon": "^5.1",
    "css-loader": "^2.1.1",
    "decanter": "^6.0.0-alpha.1",
    "drupal-behaviors-loader": "^1.0.1",
    "eslint": "^5.15.3",
    "extra-watch-webpack-plugin": "^1.0.3",
    "file-loader": "^3.0.1",
    "filemanager-webpack-plugin": "^2.0.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.1",
    "sass-lint": "^1.13",
    "sass-loader": "^7.3",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.41",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.9.0",
    "webpack-fix-style-only-entries": "^0.2.1",
    "webpack-manifest-plugin": "^2.2"
  },
