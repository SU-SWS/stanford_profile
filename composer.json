        "drupal/layout_library": "^1.0-beta1",
        "drupal/link_title_formatter": "^1.1",
        "drupal/menu_admin_per_menu": "^1.0",
        "drupal/menu_block": "dev-1.x#567becffbb0589e824fb053f15fb38a5846e7276",
        "drupal/menu_link_weight": "^1.0-beta3",
        "drupal/metatag": "^1.3",
        "drupal/node_revision_delete": "^1.0@RC",
        "drupal/paragraphs": "^1.11",
        "drupal/paragraphs_edit": "^2.0@alpha",
        "drupal/paranoia": "^1.0@alpha",
        "drupal/pathauto": "^1.6",
        "drupal/redirect": "^1.0-beta1",
        "drupal/response_code_condition": "^1.0",
        "drupal/role_delegation": "^1.0@beta",
        "drupal/seckit": "^2.0",
        "drupal/ui_patterns": "^1.0",
        "drupal/viewfield": "^3.0@beta",
        "drupal/views_block_filter_block": "^1.0@beta",
        "drupal/views_bulk_operations": "^3.6",
        "drupal/xmlsitemap": "^1.0",
        "ext-imagick": "*",
        "onlyextart/colorbox": "dev-master#e58476becbc89dc671093d1bcd9f99b2167fa8f7",
        "su-sws/jumpstart_ui": "dev-8.x-1.x",
        "su-sws/nobots": "dev-8.x-2.x",
        "su-sws/react_paragraphs": "dev-8.x-2.x",
        "su-sws/stanford_basic": "dev-8.x-4.x",
        "su-sws/stanford_date_formats": "dev-8.x-1.x",
        "su-sws/stanford_events": "dev-8.x-1.x",
        "su-sws/stanford_fields": "dev-8.x-1.x",
        "su-sws/stanford_image_styles": "dev-8.x-1.x",
        "su-sws/stanford_news": "dev-8.x-2.x",
        "su-sws/stanford_notifications": "dev-8.x-1.x",
        "su-sws/stanford_person": "dev-8.x-1.x",
        "su-sws/stanford_profile_helper": "dev-8.x-1.x",
        "su-sws/stanford_publication": "dev-8.x-1.x",
        "su-sws/stanford_ssp": "dev-8.x-1.x",
        "su-sws/stanford_text_editor": "dev-8.x-1.x"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Drupal\\stanford_profile\\": "src/"
        }
    },
